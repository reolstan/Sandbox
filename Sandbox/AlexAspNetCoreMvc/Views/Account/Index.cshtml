@{
}

<h2>@Model.Title</h2>
<h3>@Model.Description</h3> 

<div class="container">
    
    <div class="row">
        <div class="col-sm-3"><h4>First Name</h4></div>
        <div class="col-sm-3"><h4> Middle Name</h4></div>
        <div class="col-sm-3"> <h4>Last Name</h4></div>
        <div class="col-sm-3"> <h4>Age</h4></div>
    </div>
    @foreach (var acct in Model.Accounts) { 
    <div class="row">
        <div class="col-sm-3">@acct.FirstName</div>
        <div class="col-sm-3"> @acct.MiddleName</div>
        <div class="col-sm-3"> @acct.LastName</div>
        <div class="col-sm-3"> @acct.Age</div>
    </div>

    }

</div>
